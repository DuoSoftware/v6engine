<html>
  <head>
    <meta content="text/html; charset=windows-1252" http-equiv="content-type">
  </head>
  <body>
    <div ng-controller="authCtrl">
      <article class="module width_full">
        <header>
          <h3>{{pageName}}</h3>
        </header>
        <div class="module_content">
          <form class="post_message" name="Auth">
            <table width="100%">
              <tbody>
                <tr>
                  <td>User Name</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>Password</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td >Https Enabled</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>Certificate
                    <meta http-equiv="content-type" content="text/html; charset=utf-8">
                  </td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>Private Key</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>SMTP Server</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>SMTP Username</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
                <tr>
                  <td>SMTP Password</td>
                  <td> <input name="UserName" ng-bind="FileContent.UserName" type="text">
                  </td>
                </tr>
              </tbody>
            </table>
            <input value="Save" class="alt_btn" type="submit"> <input
              value="Reset" type="submit"> </form>
          </div>
      </article>
    </div>
  </body>
</html>
